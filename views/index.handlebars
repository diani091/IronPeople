<div class="container">
  <div class="row">
    {{#each clothes}}
    <div class="col-xl-3 col-lg-3 col-md-4 col-sm-6 col-6">
      <a href="/{{this.dataValues.id}}" class="block2-name dis-block s-text3 p-b-5">
        <img src="/images/{{this.dataValues.id}}.png" class="img-fluid" alt="IMG-PRODUCT">
      </a>
      <ul id="example-list" class="list-group">
        <li data-id="{{this.dataValues.id}}" class="list-group-item">
          <a href="/{{this.dataValues.id}}">{{this.dataValues.type}}</a>
        </li>

        <li class="list-group-item">
          <span class="block2-price m-text6 p-r-5">
            {{this.dataValues.regular_price}}
          </span>
        </li>

      </ul>


      <button class="flex-c-m size1 bg4 bo-rad-23 hov1 s-text1 trans-0-4">
        Add to Cart
      </button>
    </div>
    {{/each}}
  </div>
</div>
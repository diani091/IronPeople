
      <div class="container">
        <div class="row">
           {{#each clothes}}
          <div class="col-4">
            <img src="/images/" + {{this.dataValues.id}} + ".png" height="380px" width="250px" alt="IMG-PRODUCT">
            <a href="product-detail.html" class="block2-name dis-block s-text3 p-b-5">
            </a>
            <ul id="example-list" class="list-group">
              <li data-id="{{this.dataValues.id}}" class="list-group-item">
                <a href="example/{{this.dataValues.id}}">{{this.dataValues.type}}</a>
              </li>

              <li class="list-group-item">
                  <span class="block2-price m-text6 p-r-5">
              {{this.dataValues.regular_price}}
            </span>
              </li>
              
            </ul>
          

            <button class="flex-c-m size1 bg4 bo-rad-23 hov1 s-text1 trans-0-4">
              Add to Cart
            </button>
            {{/each}}
          </div>
        </div>
      </div>
